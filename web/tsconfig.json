{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "strict": false,
        "module": "esnext",
        "moduleResolution": "node",
        "isolatedModules": true,
        "jsx": "preserve"
    },
    "include": [
        "./next-env.d.ts",
        "./**/*.ts",
        "./**/*.tsx"
    ],
    "exclude": [
        "node_modules"
    ]
}